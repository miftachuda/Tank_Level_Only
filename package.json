{
  "name": "tanklevelapp",
  "version": "1.2.0",
  "description": "Realtime Tank Level App",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "make": "electron-packager ./ tankapp --platform=win32 --overwrite",
    "export": "electron-builder --win=portable",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "repository": "https://github.com/miftachuda/Tank_Level_Only",
  "keywords": [
    "Tank",
    "Level"
  ],
  "build": {
    "appId": "com.tanklevelapp.example",
    "compression": "maximum",
    "win": {
      "icon": "./src/renderer/img/icon.ivo"
    },
    "extraFiles": [
      "./src/renderer/img/datasg.json"
    ]
  },
  "author": "Miftchul Huda",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^12.2.3",
    "electron-builder": "^24.6.4",
    "electron-reloader": "^1.2.1"
  },
  "dependencies": {
    "custom-electron-titlebar": "^3.2.7",
    "electron-tabs": "^1.0.1",
    "moment": "^2.29.1",
    "node-cron": "^3.0.0",
    "xml2js": "^0.4.23"
  }
}
